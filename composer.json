{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "lexik/jwt-authentication-bundle": "^2.6",
    "ramsey/uuid": "^3.8",
    "snc/redis-bundle": "^3.0",
    "symfony/cache": "^4.4.0",
    "symfony/console": "^4.4.0",
    "symfony/dotenv": "^4.4.0",
    "symfony/flex": "^1.3.1",
    "symfony/form": "^4.4.0",
    "symfony/framework-bundle": "^4.4.0",
    "symfony/http-client": "^4.4.0",
    "symfony/messenger": "^4.4.0",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.4.0",
    "symfony/serializer-pack": "^1.0",
    "symfony/translation": "^4.4.0",
    "symfony/twig-bundle": "^4.4.0",
    "symfony/validator": "^4.4.0",
    "symfony/yaml": "^4.4.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "phpstan/phpstan": "^0.11.12",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-create-project-cmd": [
      "chmod +x ./init.sh && ./init.sh && rm ./init.sh && rm -rf ./topublic"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^4.4.0"
    }
  }
}
